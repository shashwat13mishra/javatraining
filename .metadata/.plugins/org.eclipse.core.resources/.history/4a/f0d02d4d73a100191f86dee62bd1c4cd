
public class ChemicalElementApp {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		ChemicalElement obj1= new ChemicalElement(31,"Zn","Zinc");
		System.out.println("IsMetal : "+ obj1.isMetal(obj1.atomicNumber));
		System.out.println("IsTransitionMetal : "+ obj1.isTransitionMetal(obj1.atomicNumber));
		System.out.println("IsAlkaliMetal : "+ obj1.isAlkaliMetal(obj1.atomicNumber));
		
		ChemicalElement obj2= new ChemicalElement(30,"Zn","Zinc");
		System.out.println("IsMetal : "+ obj2.isMetal(obj2.atomicNumber));
		System.out.println("IsTransitionMetal : "+ obj2.isTransitionMetal(obj2.atomicNumber));
		System.out.println("IsAlkaliMetal : "+ obj2.isAlkaliMetal(obj2.atomicNumber));
	
		System.out.println(obj1==obj2);
		System.out.println(obj1.equals(obj2));
	
	}
	

}
